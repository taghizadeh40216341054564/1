#include <iostream>
using namespace std;
int main()
{
    int a,b,c,max;
    cout<<"Please enter Your three numbers...";
    cin>>a>>b>>c;
    max=a;
            if(b>max)
                max=b;
            if (c>max)
                max=c;
            cout<<"The max is "<<max<<endl;
}
